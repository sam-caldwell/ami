version: 1.0.0
project:
  name: complex
  version: 0.1.0
toolchain:
  compiler:
    concurrency: NUM_CPU
    target: ./build
    env:
      - windows/amd64
      - linux/amd64
      - linux/arm64
      - darwin/amd64
      - darwin/arm64
  linker: {}
  linter: {}
packages:
  - main:
      name: complex-main
      version: 0.1.0
      root: ./src
      import:
        - ./vendor/alpha
  - alpha:
      name: alpha
      version: 1.0.0
      root: ./vendor/alpha
      import: []
